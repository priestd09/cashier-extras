{
    "name": "travoltron/cashier-extras",
    "description": "Adding the missing features to the fantastic Laravel Cashier package",
    "type": "library",
    "keywords": [
        "travoltron",
        "cashier-extras"
    ],
    "homepage": "https://github.com/travoltron/cashier-extras",
    "license": "MIT",
    "authors": [{
        "name": "Ben Warburton",
        "email": "ben@travoltron.com",
        "homepage": "https://github.com/travoltron/cashier-extras",
        "role": "Developer"
    }],
    "require": {
        "stripe/stripe-php": "^3.15",
        "nesbot/carbon": "~1.18",
        "doctrine/dbal": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Travoltron\\CashierExtras\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Travoltron\\CashierExtras\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "config": {
        "sort-packages": true
    }
}
